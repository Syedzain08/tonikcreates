{
  "version": 2,
  "outputDirectory": "build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/admin/config.yml",
      "dest": "/admin/config.yml"
    },
    {
      "src": "/admin$",
      "status": 301,
      "headers": { "Location": "/admin/" }
    },
    {
      "src": "/admin/",
      "dest": "/admin/index.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}